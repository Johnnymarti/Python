def hasVowels(word):
  found = word.find('a')
  if(found != -1):
    return True

  found = word.find('e')
  if(found != -1):
    return True

  found = word.find('i')
  if(found != -1):
    return True

  found = word.find('o')
  if(found != -1):
    return True 

  found = word.find('U')
  if(found != -1):
    return True
  else:
    return False
variable = "Hello World"
  
if(hasVowels(variable)!= False):
    print("vowel")

